{
  "name": "rsshub",
  "version": "1.0.0",
  "description": "Make RSS Great Again!",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "node lib/index.js",
    "dev": "cross-env NODE_ENV=dev nodemon --inspect lib/index.js",
    "lint": "eslint lib --fix --no-error-on-unmatched-pattern",
    "test": "cross-env NODE_ENV=test jest -w 4 --forceExit --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soontao/RSSHub.git"
  },
  "keywords": [
    "RSS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Soontao/RSSHub/issues"
  },
  "homepage": "https://github.com/Soontao/RSSHub#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^26.0.24",
    "@types/koa": "^2.15.0",
    "cross-env": "^7.0.3",
    "entities": "^2.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "mockdate": "3.0.5",
    "nock": "^13.5.4",
    "supertest": "^7.0.0",
    "uuid": "^9.0.1"
  },
  "dependencies": {
    "@koa/router": "^12.0.1",
    "@newdash/newdash": "^5.22.1",
    "@node-rs/jieba": "^1.10.3",
    "art-template": "4.13.2",
    "cheerio": "1.0.0-rc.12",
    "chrono-node": "^2.7.5",
    "crypto-js": "^4.2.0",
    "csv": "^6.3.8",
    "currency-symbol-map": "4.0.4",
    "dayjs": "^1.11.11",
    "dotenv": "8.2.0",
    "etag": "1.8.1",
    "got": "~11.8.6",
    "he": "1.2.0",
    "iconv-lite": "0.6.3",
    "is-localhost-ip": "1.4.0",
    "js-jieba": "^1.0.12",
    "jsdom": "^24.0.0",
    "json-bigint": "1.0.0",
    "json5": "^2.2.3",
    "koa": "^2.15.3",
    "koa-basic-auth": "4.0.0",
    "koa-favicon": "2.1.0",
    "koa-mount": "4.0.0",
    "lz-string": "1.4.4",
    "markdown-it": "^13.0.2",
    "marked": "^12.0.2",
    "module-alias": "2.2.2",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "pidusage": "2.0.21",
    "plist": "^3.1.0",
    "query-string": "^6.14.1",
    "redis": "3.1.2",
    "require-all": "3.0.0",
    "rss-parser": "^3.13.0",
    "tiny-async-pool": "^1.3.0",
    "tough-cookie": "^4.1.4",
    "turndown": "^7.1.3",
    "winston": "3.3.3"
  },
  "jest": {
    "testTimeout": 45000,
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!lib/routes/**/*.js",
      "!lib/customize/**/*.js"
    ],
    "bail": true,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/lib/$1"
    }
  }
}
