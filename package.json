{
  "name": "rsshub",
  "version": "1.0.0",
  "description": "Make RSS Great Again!",
  "private": true,
  "engines": {
    "node": ">=10 <=14"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "node lib/index.js",
    "dev": "cross-env NODE_ENV=dev nodemon --inspect lib/index.js",
    "lint": "eslint lib --fix --no-error-on-unmatched-pattern",
    "test": "cross-env NODE_ENV=test jest -w 4 --forceExit --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soontao/RSSHub.git"
  },
  "keywords": [
    "RSS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Soontao/RSSHub/issues"
  },
  "homepage": "https://github.com/Soontao/RSSHub#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.29",
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.3",
    "cross-env": "^7.0.3",
    "entities": "^2.2.0",
    "eslint": "^7.30.0",
    "jest": "^26.6.3",
    "mockdate": "3.0.2",
    "nock": "^13.1.0",
    "nodemon": "^2.0.9",
    "staged-git-files": "1.2.0",
    "string-width": "4.2.0",
    "supertest": "^6.1.3",
    "uuid": "^8.3.2",
    "yorkie": "2.0.0"
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "@newdash/newdash": "^5.19.0",
    "@postlight/mercury-parser": "2.2.0",
    "@sentry/node": "^5.29.2",
    "aes-js": "3.1.2",
    "art-template": "4.13.2",
    "cheerio": "^1.0.0-rc.10",
    "chrono-node": "^2.3.0",
    "crypto-js": "4.0.0",
    "currency-symbol-map": "4.0.4",
    "dayjs": "^1.10.5",
    "dotenv": "8.2.0",
    "etag": "1.8.1",
    "fanfou-sdk": "4.2.0",
    "git-rev-sync": "3.0.1",
    "googleapis": "^66.0.0",
    "got": "^11.8.2",
    "he": "1.2.0",
    "hooman": "1.2.6",
    "https-proxy-agent": "5.0.0",
    "iconv-lite": "0.6.2",
    "is-localhost-ip": "1.4.0",
    "jsdom": "16.4.0",
    "json-bigint": "1.0.0",
    "json5": "2.1.3",
    "koa": "2.13.0",
    "koa-basic-auth": "4.0.0",
    "koa-favicon": "2.1.0",
    "koa-mount": "4.0.0",
    "lz-string": "1.4.4",
    "mailparser": "^3.2.0",
    "markdown-it": "^12.1.0",
    "module-alias": "2.2.2",
    "moment": "^2.29.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "parse-torrent": "^9.1.3",
    "pidusage": "2.0.21",
    "plist": "^0.3.1",
    "puppeteer": "^5.5.0",
    "query-string": "^6.13.8",
    "redis": "3.0.2",
    "require-all": "3.0.0",
    "rss-parser": "^3.12.0",
    "socks-proxy-agent": "5.0.0",
    "tiny-async-pool": "^1.2.0",
    "tough-cookie": "4.0.0",
    "tunnel": "0.0.6",
    "twit": "2.2.11",
    "winston": "3.3.3"
  },
  "jest": {
    "testTimeout": 60000,
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!lib/routes/**/*.js"
    ],
    "bail": true,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/lib/$1"
    }
  }
}