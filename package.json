{
  "name": "rsshub",
  "version": "1.0.0",
  "description": "Make RSS Great Again!",
  "private": true,
  "engines": {
    "node": ">=10 <=14"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "node lib/index.js",
    "dev": "cross-env NODE_ENV=dev nodemon --inspect lib/index.js",
    "lint": "eslint lib --fix --no-error-on-unmatched-pattern",
    "test": "cross-env NODE_ENV=test jest -w 4 --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theosun/RSSHub.git"
  },
  "keywords": [
    "RSS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theosun/RSSHub/issues"
  },
  "homepage": "https://github.com/theosun/RSSHub#readme",
  "devDependencies": {
    "@types/cheerio": "0.22.21",
    "@types/got": "^9.6.11",
    "@types/jest": "^26.0.12",
    "@types/koa": "2.11.4",
    "cross-env": "7.0.2",
    "entities": "2.0.3",
    "eslint": "7.7.0",
    "jest": "26.4.2",
    "mockdate": "3.0.2",
    "nock": "13.0.4",
    "nodemon": "2.0.4",
    "remark": "12.0.1",
    "remark-frontmatter": "2.0.0",
    "remark-pangu": "2.1.1",
    "remark-preset-prettier": "0.4.0",
    "staged-git-files": "1.2.0",
    "string-width": "4.2.0",
    "supertest": "4.0.2",
    "yorkie": "2.0.0"
  },
  "dependencies": {
    "@koa/router": "9.4.0",
    "@postlight/mercury-parser": "2.2.0",
    "@sentry/node": "5.22.3",
    "aes-js": "3.1.2",
    "art-template": "4.13.2",
    "cheerio": "1.0.0-rc.3",
    "chrono-node": "2.1.8",
    "co-redis": "2.1.1",
    "crypto-js": "4.0.0",
    "currency-symbol-map": "4.0.4",
    "dayjs": "1.8.34",
    "dotenv": "8.2.0",
    "emailjs-imap-client": "3.1.0",
    "etag": "1.8.1",
    "fanfou-sdk": "4.2.0",
    "git-rev-sync": "3.0.1",
    "googleapis": "59.0.0",
    "got": "11.5.2",
    "he": "1.2.0",
    "hooman": "1.2.6",
    "https-proxy-agent": "5.0.0",
    "iconv-lite": "0.6.2",
    "is-localhost-ip": "1.4.0",
    "jsdom": "16.4.0",
    "json-bigint": "1.0.0",
    "json5": "2.1.3",
    "koa": "2.13.0",
    "koa-basic-auth": "4.0.0",
    "koa-favicon": "2.1.0",
    "koa-mount": "4.0.0",
    "lru-cache": "6.0.0",
    "lz-string": "1.4.4",
    "mailparser": "2.8.1",
    "markdown-it": "11.0.0",
    "module-alias": "2.2.2",
    "moment": "2.27.0",
    "parse-torrent": "7.1.3",
    "pidusage": "2.0.21",
    "plist": "3.0.1",
    "puppeteer": "5.2.1",
    "query-string": "6.13.1",
    "redis": "3.0.2",
    "require-all": "3.0.0",
    "rss-parser": "3.9.0",
    "socks-proxy-agent": "5.0.0",
    "tiny-async-pool": "1.1.0",
    "tough-cookie": "4.0.0",
    "tunnel": "0.0.6",
    "twit": "2.2.11",
    "winston": "3.3.3"
  },
  "jest": {
    "testTimeout": 60000,
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!lib/routes/**/*.js"
    ],
    "bail": true,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/lib/$1"
    }
  }
}
